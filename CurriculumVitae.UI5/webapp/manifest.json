{
  "sap.app": {
    "id": "my.ui5.app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "My UI5 App",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "my.ui5.app.view.MainView",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.105.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    }
  }
}
